<% if(typeof errors !=='undefined' ){ %>
  <% errors.forEach(function(error){ %>
    <div class="alert alert-error  " role="alert">
      <%= error.msg %>
    </div>
  <% }); %>
<% } %>

<% if(successMessage !='' ){ %>
  <div class="alert alert-success  " role="alert">
    <%= successMessage %>
  </div>
<% } %>

<% if(errorMessage !='' ){ %>
  <div class="alert alert-error  " role="alert">
    <%= errorMessage %>
  </div>
<% } %>

<% if(error !='' ){ %>
  <div class="alert alert-error  " role="alert">
    <%= error %>
  </div>
<% } %>

<% if(warningMessage!='' ){ %>
  <div class="alert alert-warning  " role="alert">
    <%= warningMessage %>
  </div>
<% } %>